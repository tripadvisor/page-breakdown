{
  "manifest_version": 2,
  "name": "Page Breakdown Tool",
  "description": "A utility to show the placements, widgets, and components that make up a TA consumer page",
  "icons": {
    "16": "ollie-16.png",
    "48": "ollie-48.png",
    "128": "ollie-128.png"
  },
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent"
  ]
}
 
